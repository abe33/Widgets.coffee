container:"filepickers"
builder:
  title:"FilePickers"
  cls:this["FilePicker"]
  rows:1
  cols:3
  tableClass:"float dummy"
  cells:(opts)->
    states = [ null, "readonly", "disabled" ]
    opts.widget.set states[ opts.col ], true if states[ opts.col ]?
  columnHeaders:(opts)->
    [ "Normal", "Readonly", "Disabled" ][opts.col]
